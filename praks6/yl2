#!/bin/bash

echo "Sisesta reisijate arv: "
read reisijad

echo "Sisesta istekohtade arv: "
read istekohad

#arvutab mitu bussi saab t채is 
bussid=$(($reisijad / $istekohad))

#arvutab mitu reisijat j채채b maha
maha=$(($reisijad % $istekohad))

if test $maha -gt 0
then
	bussid=$(($bussid + 1))
fi

echo "Kokku on vaja $bussid t채isbussi"

